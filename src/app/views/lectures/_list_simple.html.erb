<section>
<table class="table table-striped">
  <thead>
  <tr>
    <th>Lecture</th>
    <th>Maintainer</th>
  </tr>
  </thead>

<% lectures.each do |lecture| %>
  <tr data-id="<%= lecture.id%>" class="lecture">
    <td class="lecture"> <%= link_to lecture.name, lecture %></td>
    <td class=""> <em>unmaintained </em></td>
  </tr>
<% end %>
</table>
</section>
